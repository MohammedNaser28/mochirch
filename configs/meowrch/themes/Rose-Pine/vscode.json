{
  // Name and basic theme metadata
  "name": "Rose Pine Classic",
  "type": "dark",
  "semanticHighlighting": true,

  // Semantic token overrides (optional customization)
  "semanticTokenColors": {
    "variable": { "foreground": "#e0def4" },  // Default variable color
    "function": { "foreground": "#9ccfd8" },  // Function names
    "parameter": { "foreground": "#f6c177" }, // Function parameters
    "class": { "foreground": "#c4a7e7" }      // Classes and types
  },

  // Token-based syntax coloring
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e6a86",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#eb6f92"
      }
    },
    {
      "name": "Function Names",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#e0def4"
      }
    },
    {
      "name": "Types and Classes",
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#908caa"
      }
    },
    {
      "name": "Invalid / Deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#26233a",
        "background": "#eb6f92"
      }
    }
  ],

  // UI colors (workbench, editor, panel, etc.)
  "colors": {
    // General UI
    "foreground": "#e0def4",
    "editor.background": "#191724",
    "editor.foreground": "#e0def4",
    "editorLineNumber.foreground": "#6e6a86",
    "editorLineNumber.activeForeground": "#f6c177",

    // Cursor & Selection
    "editorCursor.foreground": "#f6c177",
    "editor.selectionBackground": "#2a273f",
    "editor.inactiveSelectionBackground": "#2a273f88",

    // Bracket Match Highlight
    "editorBracketMatch.background": "#403d52",
    "editorBracketMatch.border": "#eb6f92",

    // Gutter
    "editorGutter.background": "#191724",
    "editorGutter.modifiedBackground": "#9ccfd8",
    "editorGutter.addedBackground": "#31748f",
    "editorGutter.deletedBackground": "#eb6f92",

    // Tabs
    "tab.activeBackground": "#191724",
    "tab.inactiveBackground": "#1f1d2e",
    "tab.activeForeground": "#e0def4",
    "tab.inactiveForeground": "#6e6a86",
    "tab.border": "#1f1d2e",

    // Activity Bar
    "activityBar.background": "#1f1d2e",
    "activityBar.foreground": "#e0def4",
    "activityBarBadge.background": "#f6c177",
    "activityBarBadge.foreground": "#191724",

    // Side Bar
    "sideBar.background": "#1f1d2e",
    "sideBar.foreground": "#e0def4",
    "sideBarSectionHeader.foreground": "#f6c177",

    // Status Bar
    "statusBar.background": "#1f1d2e",
    "statusBar.foreground": "#e0def4",
    "statusBar.debuggingBackground": "#eb6f92",
    "statusBar.debuggingForeground": "#191724",

    // Panel
    "panel.background": "#1f1d2e",
    "panel.border": "#2a273f",

    // Terminal
    "terminal.background": "#191724",
    "terminal.foreground": "#e0def4",
    "terminalCursor.foreground": "#f6c177",
    "terminal.ansiBlack": "#26233a",
    "terminal.ansiRed": "#eb6f92",
    "terminal.ansiGreen": "#31748f",
    "terminal.ansiYellow": "#f6c177",
    "terminal.ansiBlue": "#9ccfd8",
    "terminal.ansiMagenta": "#c4a7e7",
    "terminal.ansiCyan": "#ebbcba",
    "terminal.ansiWhite": "#e0def4",
    "terminal.ansiBrightBlack": "#6e6a86",
    "terminal.ansiBrightRed": "#eb6f92",
    "terminal.ansiBrightGreen": "#31748f",
    "terminal.ansiBrightYellow": "#f6c177",
    "terminal.ansiBrightBlue": "#9ccfd8",
    "terminal.ansiBrightMagenta": "#c4a7e7",
    "terminal.ansiBrightCyan": "#ebbcba",
    "terminal.ansiBrightWhite": "#ffffff"
  }
}
